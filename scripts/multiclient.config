#!/bin/bash

################################
# multiclient sysbench config params
################################
# multiclient setup related

TARGET_VOLUME='/home'
# specify this when you want to run sysbench on same volume
# (in that case, this would make different folders on that volume for each client,
REINSTALL_OPTION=1
# supply =1, if you wanna remove and reset previous mariadb/sysbench traces
# to store their respective sysbench log files)
AIO_MODE=native
# options available: native OR threads
CLIENT_LOGIN_USER=root

# leave these settings unachnged
UTIL_NAMES=sysbench-0.4.12.tar.gz,my.cnf.example
SCRIPT_NAMES=setup_sysbench.sh,prepare_sysbench_tests.sh,profit3_sysbench.sh,run-sysbench.sh,run-sysbench-pbench.sh,set_result_name.sh
MACHINE_IP=$(hostname -I | awk '{print $1}' | xargs echo -n)

# change the download link to sysbench as required
DOWNLOAD_LINK=http://pkgs.fedoraproject.org/repo/pkgs/sysbench/sysbench-0.4.12.tar.gz/3a6d54fdd3fe002328e4458206392b9d/sysbench-0.4.12.tar.gz

###################################################
# sysbench execution related
TIME=600
# recommended - 600 (for better test results)
THREADS="1"
# recommended "1 2 4 8" (depending on your vcpu allocations to the VM / cpu on clients)
# use this to specify thread counts for various iterations.
OLTP_TABLE_SIZE=6000
# recommended 60000000
MULTICLIENT_ROOT_DIR=/opt/multiclient_sysbench/
RESULTS_DIR=${MULTICLIENT_ROOT_DIR%/}/results/
E_LOG_FILENAME="$AIO_MODE"_e.log

###################################################
# pbench related
ENABLE_PBENCH=0
CONFIG_NAME='sysbench_MariaDB_MultiClient_AIO'

###################################################
# define mysql auth
MYSQL_USERNAME=root
MYSQL_PASS=90feet-

# leave old mysql pass blank since we don't change it.
MYSQL_PASS_OLD=""

###################################################
